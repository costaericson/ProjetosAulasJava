package oo.composicao;

public class Item {
	String nome;
	int quantidade;
	double preco;
	
	public Item(String nome, int quantidade, double preco) {
		// TODO Auto-generated constructor stub
		this.nome = nome;
		this.quantidade = quantidade;
		this.preco = preco;
		//Contrutor criado para forçar a inicializacaso desses valores

	}
	
	

}
